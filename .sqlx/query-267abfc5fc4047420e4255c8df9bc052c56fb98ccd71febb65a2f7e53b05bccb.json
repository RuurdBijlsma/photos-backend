  "query": "\n            UPDATE app_user\n            SET\n                name = COALESCE($1, name),\n                email = COALESCE($2, email),\n                password = COALESCE($3, password),\n                role = COALESCE($4, role),\n                media_folder = COALESCE($5, media_folder),\n                updated_at = now()\n            WHERE id = $6\n            RETURNING\n                id,\n                created_at,\n                updated_at,\n                email,\n                name,\n                media_folder,\n                role as \"role: UserRole\"\n            ",
