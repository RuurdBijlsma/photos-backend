  "query": "\n            SELECT a.id, owner_id, name, description, thumbnail_id, is_public, created_at, updated_at\n            FROM album a\n            JOIN album_collaborator ac ON a.id = ac.album_id\n            WHERE ac.user_id = $1\n            ORDER BY a.updated_at DESC\n            ",
