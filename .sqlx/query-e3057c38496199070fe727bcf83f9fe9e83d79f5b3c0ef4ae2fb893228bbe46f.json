{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                a.id,\n                a.owner_id,\n                a.name,\n                a.description,\n                a.thumbnail_id,\n                a.is_public,\n                a.created_at,\n                a.updated_at,\n                COUNT(mi.id)::INT AS \"media_count!\"\n            FROM album a\n            LEFT JOIN album_media_item ami ON a.id = ami.album_id\n            LEFT JOIN media_item mi ON ami.media_item_id = mi.id AND mi.deleted = false\n            WHERE a.id = $1\n            GROUP BY a.id\n            ",
