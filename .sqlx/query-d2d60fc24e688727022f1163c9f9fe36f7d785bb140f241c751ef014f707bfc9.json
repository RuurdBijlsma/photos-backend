  "query": "\n        INSERT INTO app_user (email, name, password, role)\n        SELECT\n            $1, -- payload.email\n            $2, -- payload.name\n            $3, -- hashed password\n            CASE\n                WHEN NOT EXISTS (SELECT 1 FROM app_user) THEN 'admin'::user_role\n                ELSE 'user'::user_role\n            END\n        RETURNING id, email, name, media_folder, role as \"role: UserRole\", created_at, updated_at\n        ",
